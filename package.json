{
   "name": "bknd-ui-local-code-remote",
   "version": "0.0.0",
   "private": true,
   "type": "module",
   "scripts": {
      "deploy": "wrangler deploy --env production",
      "predeploy": "npm run bknd:config",
      "dev": "vite",
      "dev:cf": "wrangler dev",
      "preview": "wrangler dev --env production",
      "bknd": "PROXY=1 node --experimental-strip-types node_modules/.bin/bknd",
      "bknd:types": "npm run bknd -- types",
      "bknd:config": "npm run bknd -- config --pretty --out appconfig.json",
      "bknd:secrets": "npm run bknd -- secrets --format env --template --out .env.example",
      "bknd:assets": "npm run bknd -- copy-assets --out public",
      "typegen": "wrangler types && npm run bknd:types",
      "predev": "npm run bknd:config",
      "postinstall": "npm run bknd -- config --pretty --default --out appconfig.json && npm run typegen && npm run bknd:secrets && npm run bknd:assets"
   },
   "dependencies": {
      "bknd": "^0.18.0-rc.2"
   },
   "devDependencies": {
      "@cloudflare/vite-plugin": "^1.12.3",
      "typescript": "^5.9.2",
      "vite": "^7.1.4",
      "wrangler": "^4.28.1"
   }
}
